<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cocoawork.appstore.mapper.GenreMapper">
    <insert id="addGenre" parameterType="com.cocoawork.appstore.entity.Genre">
        INSERT INTO genre(genre_id, `name`, url) VALUES (#{genreId}, #{name}, #{url})
    </insert>
</mapper>
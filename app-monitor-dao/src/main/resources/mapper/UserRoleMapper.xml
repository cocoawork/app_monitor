<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.cocoawork.monitor.dao.mapper.UserRoleMapper">

    <select id="selectByUserId" resultType="top.cocoawork.monitor.dao.entity.UserRole">
        select * from t_user_role where user_id = #{userId} and is_delete = 0
    </select>
</mapper>